/**index.wxss**/
.index-swiper swiper {
  width: 750rpx;
  height: 340rpx;
}

.index-swiper swiper image {
  width: 100%;
}

.navMenu {
  width: 750rpx;
  padding: 20rpx 0;
  display: flex;
}

.navMenu navigator {
  flex: 1;
  display: flex;
  justify-content: center;
}

.navMenu navigator image {
  width: 130rpx;
}

.floor-list .floor {
  margin: 20rpx 0 6rpx;
}

.floor-list .floor image {
  width: 100%;
}

.floor-list .product {
  --floorHeight: 27.72711vw;
  display: grid;
  grid-template-rows: 27.72711vw 27.72711vw;
  grid-template-columns: 1fr 1fr 1fr;
  grid-gap: 10rpx 10rpx;
}

.floor-list .product navigator:nth-child(1) {
  grid-area: 1 / 1 / 3 / 2;
}

.floor-list .product navigator image {
  width: 100%;
  height: 100%;
}
