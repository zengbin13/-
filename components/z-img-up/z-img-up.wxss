/* components/z-img-up/z-img-up.wxss */
.img-up {
  width: 90rpx;
  height: 90rpx;
  position: relative;
  box-sizing: border-box;
  border: 1px solid rgba(102, 102, 102, 0.582);
  border-radius: 10rpx;
  margin: 10rpx;
}

.img-up image {
  width: 100%;
  height: 100%;
}

.img-up icon {
  position: absolute;
  top: -19rpx;
  right: -21rpx;
  background-color: #fff;
}
